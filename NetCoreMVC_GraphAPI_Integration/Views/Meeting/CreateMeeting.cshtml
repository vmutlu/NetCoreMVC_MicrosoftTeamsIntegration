<form method="post" action="CreateMeeting">
    <div class="row" style="margin:10px">
        <label for="Summary" class="col-sm-3">Subject:</label>
        <input type="text" class="col-sm-6 form-control" name="Subject" value="Example Subject" autocomplete="off" />
    </div>
    <div class="row" style="margin:10px">
        <label for="Description" class="col-sm-3">Content:</label>
        <input type="datetime" class="col-sm-6 form-control" name="Body.Content" value="Example Content" autocomplete="off" />
    </div>
    <div class="row" style="margin:10px">
        <label for="Start.DateTime" class="col-sm-3">Start Date Time:</label>
        <input type="datetime" class="col-sm-6 form-control" name="Start.DateTime" value="2021-08-20 10:00" autocomplete="off" />
    </div>
    <div class="row" style="margin:10px">
        <label for="End.DateTime" class="col-sm-3">End Date Time:</label>
        <input type="datetime" class="col-sm-6 form-control" name="End.DateTime" value="2021-08-20 11:00" autocomplete="off" />
    </div>
    <div class="row col-sm-3" style="margin:10px">
        <input type="submit" value="Create Meeting" class="btn btn-success" />
    </div>
</form>
